on post-fs-data
    # create socket for cashsvr
    mkdir /dev/socket/cashsvr 0755 system system

on property:sys.boot_completed=1
    start cashsvr

on property:vendor.cash.restart=1
    setprop vendor.cash.restart 0
    restart cashsvr

service cashsvr /vendor/bin/cashsvr
    class main
    user root
    group system input
    disabled
